{
  "name": "vscode-copilot-exporter",
  "displayName": "GitHub Copilot Chat Exporter",
  "description": "Export your GitHub Copilot chat conversations as clean, readable JSON files. Perfect for backing up conversations or analyzing development patterns.",
  "version": "0.1.1",
  "publisher": "fengzehan",
  "icon": "logo/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Fzzzhan/vscode-copilot-exporter"
  },
  "bugs": {
    "url": "https://github.com/Fzzzhan/vscode-copilot-exporter/issues"
  },
  "homepage": "https://github.com/Fzzzhan/vscode-copilot-exporter#readme",
  "keywords": [
    "copilot",
    "chat",
    "export",
    "github",
    "ai",
    "conversation",
    "backup",
    "json"
  ],
  "license": "MIT",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.70.0",
    "typescript": "^4.9.5",
    "vsce": "1.103.1"
  },
  "overrides": {
    "cheerio": "1.0.0-rc.12",
    "undici": "5.28.4"
  },
  "contributes": {
    "commands": [
      {
        "command": "copilot-exporter.exportWorkspaceHistory",
        "title": "Export Copilot Chat: Export workspace Copilot history",
        "icon": "$(export)"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "copilot-exporter.exportWorkspaceHistory",
          "when": "true"
        }
      ]
    }
  }
}
